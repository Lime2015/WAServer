
<mapping>
<description>A mapping file for our Address Book application</description>

<class name="vo.Assemblyman">
  <field name="manId" type="integer">
    <bind-xml name="manId" node="attribute" />
  </field>
  <field name="name" type="string" />
  <field name="imgUrl" type="string" />
  <field name="orgImg" type="string" />
  <field name="partyId" type="integer" />
  <field name="partyName" type="string" />
  <field name="localConstituency" type="string" />
</class>

<class name="AssemblymanList">
  <field name="assemblymanListName" type="string">
    <bind-xml name="assemblymanListName" node="attribute" />
  </field>
  <field name="assemblyman" type="Assemblyman" collection="collection" />
</class>

</mapping>